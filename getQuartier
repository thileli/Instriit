function listbox_mois($mois='')
{        
global $listemois;
    for ($i=1;$i<13;$i++)
    {
    $j = $i-1;
    if ($i<10) $i='0'.$i;
    echo '<option value="',$i,'"';
    if($i==$mois){
    echo ' selected';
    }
    echo '>',$listemois[$j],'</option>';
    }
}
$id_mois=date('m');
echo '<form>
<table>
<tr><td>
       
        <select name="mois_deb">',listbox_mois($id_mois),'</select>
<br /><br /></td></tr>
</table>
</form>';
